{
	"name": "baileys-store-test-app",
	"version": "2.0.0",
	"description": "Test application for baileys-store with Svelte UI",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"type-check": "svelte-kit sync && tsc --noEmit",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --write ./src",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"biome:check": "biome check ./src",
		"biome:fix": "biome check --write ./src"
	},
	"dependencies": {
		"@rodrigogs/baileys-store": "file:../",
		"baileys-v6": "npm:@whiskeysockets/baileys@^6.7.21",
		"baileys-v7": "npm:@whiskeysockets/baileys@^7.0.0-rc.9",
		"pino": "^9.6.0",
		"qrcode": "^1.5.3"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.10",
		"@types/node": "^22.15.29",
		"@types/qrcode": "^1.5.5",
		"svelte": "^5.34.7",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.8.3",
		"vite": "^6.3.5"
	},
	"engines": {
		"node": ">=20.0.0"
	}
}
